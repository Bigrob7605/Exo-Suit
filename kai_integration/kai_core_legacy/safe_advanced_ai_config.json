  "recursive_audit": false,
  "stability_mode": true,
  "prevent_self_modification": true,
  "read_only_mode": false,
  "max_agent_mutations": 0,
  "genome_lock": true,
  "safety_system": {
    "enabled": true,
    "level": "maximum",
    "protected_files": [
