{
  "mode": "autonomous",
  "llm_provider": "local",
  "auto_merge": false,
  "challenge_self": true,
  "max_cycles_per_day": 10,
  "domains_to_focus": ["psychology", "social", "coding", "math", "physics", "biology"],
